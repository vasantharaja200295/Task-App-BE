{
    "builds":[
        {
            "src":"main.py",
            "use":"@vercel/python"
        }
    ],
    "crons":[
        {
            "path":"/api/cron/task-reminder",
            "schedule":"0 5 * * *"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "main.py"
        }
    ]
}